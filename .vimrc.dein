if &compatible
  set nocompatible               " Be iMproved
endif

" Required:
set runtimepath^=$HOME/.vim/repos/github.com/Shougo/dein.vim

" Required:
call dein#begin(expand($HOME.'/.vim'))

" Let dein manage dein
